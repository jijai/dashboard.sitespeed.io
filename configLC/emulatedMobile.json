{
  "extends": "/config/secrets.json",
  "browsertime": {
    "connectivity": {
      "engine": "throttle",
      "profile": "4g"
    },
    "mobile": true,
    "visualElements": true,
    "iterations": 5,
    "browser": "chrome",
    "video": true,
    "visualMetrics": true,
    "visualMetricsPortable": true,
    "chrome": {
      "CPUThrottlingRate": 5,
      "collectConsoleLog": true,
      "includeResponseBodies": "html",
      "enableTraceScreenshots": true,
      "args": [
        "disable-features=PaintHoldingCrossOrigin"
      ]
    },
    "videoParams": {
      "thumbsize": 600
    }
  },
  "utc": true,
  "cpu": true,
  "gzipHAR": true,
  "html": {
    "fetchHARFiles": true,
    "showScript": true,
    "compareURL": "https://compare.sitespeed.io/"
  },
  "mobile": true,
  "screenshot": {
    "type": "jpg"
  },
  "copyLatestFilesToBase": true,
  "copyLatestFilesToBaseGraphiteNamespace": true,
  "graphite": {
    "namespace": "sitespeed_io.emulatedMobile",
    "addSlugToKey": true
  }
}
